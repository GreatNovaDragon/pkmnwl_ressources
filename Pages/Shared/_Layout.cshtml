<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - pkmnWildLife</title>


    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">

    <link rel="stylesheet" href="icons/type/style.css"/>

    <script
        src="https://code.jquery.com/jquery-3.7.1.js"
        integrity="sha256-eKhayi8LEQwp4NKxN+CfCh+3qOVUtJn3QNZ0TciWLP4="
        crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://cdn.datatables.net/1.13.6/css/jquery.dataTables.css"/>

    <script src="https://cdn.datatables.net/1.13.6/js/jquery.dataTables.js"></script>

</head>
<body>
<header>
    <div class="w3-bar w3-black">

        <a class="w3-bar-item w3-button w3-mobile" asp-area="" asp-page="/Index">Home</a>
        <a class="w3-bar-item w3-button w3-mobile" asp-area="" asp-page="/pokemon/Index">Pokédex</a>
        <a class="w3-bar-item w3-button w3-mobile" asp-area="" asp-page="/moves/Index">AngriffsDex</a>
        <a class="w3-bar-item w3-button w3-mobile" asp-area="" asp-page="/abilities/Index">FähigkeitenDex</a>
        <a class="w3-bar-item w3-button w3-mobile" asp-area="" asp-page="/traits/Index">Traits</a>
    </div>
</header>
<div class="w3-container">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>

<footer class="w3-container w3-teal">
    <div class="container">
        &copy; 2023 - pkmnWildLife - <a asp-area="" asp-page="/Privacy">Privacy</a>
    </div>
</footer>
@await RenderSectionAsync("Scripts", false)
</body>
</html>